# Darwin Configuration Template
# Copy this to ~/.config/nix/darwin-config.nix and customize for your machine

{
  # Your username on this machine
  username = "your-username";
  
  # Your email address
  email = "your-email@example.com";
  
  # This machine's hostname
  hostname = "your-hostname";
  
  # System architecture (aarch64-darwin for Apple Silicon, x86_64-darwin for Intel)
  system = "aarch64-darwin";
}